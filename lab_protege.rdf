<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns="http://www.example.org/bird#">

  <!-- Ontology Header -->
  <owl:Ontology rdf:about="http://www.example.org/bird">
    <rdfs:label>Bird Ontology</rdfs:label>
    <rdfs:comment>An ontology describing bird species, their characteristics, habitats, and behaviors in aerial and terrestrial environments.</rdfs:comment>
    <owl:versionInfo>1.0</owl:versionInfo>
  </owl:Ontology>

  <!-- Classes -->
  
  <!-- Root class for all birds -->
  <owl:Class rdf:ID="Bird">
    <rdfs:label>Bird</rdfs:label>
    <rdfs:comment>Warm-blooded vertebrate animals characterized by feathers, wings, beaks, and the ability to fly.</rdfs:comment>
  </owl:Class>

  <!-- Bird categories by habitat -->
  <owl:Class rdf:ID="ForestBird">
    <rdfs:label>Forest Bird</rdfs:label>
    <rdfs:comment>Birds that live in forest environments such as woodlands, rainforests, and wooded areas.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Bird"/>
  </owl:Class>

  <owl:Class rdf:ID="SeaBird">
    <rdfs:label>Sea Bird</rdfs:label>
    <rdfs:comment>Birds that live in marine environments such as oceans and coastal areas.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Bird"/>
  </owl:Class>

  <!-- Bird categories by feeding behavior -->
  <owl:Class rdf:ID="CarnivorousBird">
    <rdfs:label>Carnivorous Bird</rdfs:label>
    <rdfs:comment>Birds that primarily feed on other animals, including fish, small mammals, and insects.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Bird"/>
  </owl:Class>

  <owl:Class rdf:ID="HerbivorousBird">
    <rdfs:label>Herbivorous Bird</rdfs:label>
    <rdfs:comment>Birds that primarily feed on plants, seeds, fruits, and nectar.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Bird"/>
  </owl:Class>

  <owl:Class rdf:ID="OmnivorousBird">
    <rdfs:label>Omnivorous Bird</rdfs:label>
    <rdfs:comment>Birds that feed on both plants and animals, including insects, seeds, and small prey.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Bird"/>
  </owl:Class>

  <!-- Terrestrial Environment classes -->
  <owl:Class rdf:ID="Habitat">
    <rdfs:label>Habitat</rdfs:label>
    <rdfs:comment>The natural environment where birds live, nest, and reproduce.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:ID="Ocean">
    <rdfs:label>Ocean</rdfs:label>
    <rdfs:comment>Large body of saltwater that covers most of the Earth's surface, habitat for seabirds.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Habitat"/>
  </owl:Class>

  <owl:Class rdf:ID="Forest">
    <rdfs:label>Forest</rdfs:label>
    <rdfs:comment>Large area covered with trees and undergrowth, providing nesting sites and food for many birds.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Habitat"/>
  </owl:Class>

  <owl:Class rdf:ID="Mountain">
    <rdfs:label>Mountain</rdfs:label>
    <rdfs:comment>High elevation landforms providing habitat for specialized bird species.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Habitat"/>
  </owl:Class>

  <!-- Object Properties -->
  
  <owl:ObjectProperty rdf:ID="livesIn">
    <rdfs:label>lives in</rdfs:label>
    <rdfs:comment>Describes where a bird lives and inhabits.</rdfs:comment>
    <rdfs:domain rdf:resource="#Bird"/>
    <rdfs:range rdf:resource="#Habitat"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="feedsOn">
    <rdfs:label>feeds on</rdfs:label>
    <rdfs:comment>Describes what a bird eats in its environment.</rdfs:comment>
    <rdfs:domain rdf:resource="#Bird"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="flockssWith">
    <rdfs:label>flocks with</rdfs:label>
    <rdfs:comment>Describes flocking behavior among birds.</rdfs:comment>
    <rdfs:domain rdf:resource="#Bird"/>
    <rdfs:range rdf:resource="#Bird"/>
  </owl:ObjectProperty>

  <!-- Datatype Properties -->
  
  <owl:DatatypeProperty rdf:ID="wingSpan">
    <rdfs:label>wing span</rdfs:label>
    <rdfs:comment>The wingspan of the bird measured in centimeters.</rdfs:comment>
    <rdfs:domain rdf:resource="#Bird"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:ID="flightAltitude">
    <rdfs:label>flight altitude</rdfs:label>
    <rdfs:comment>The typical altitude in meters where the bird flies.</rdfs:comment>
    <rdfs:domain rdf:resource="#Bird"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:ID="nestingHeight">
    <rdfs:label>nesting height</rdfs:label>
    <rdfs:comment>The preferred height in meters above ground for the bird nest.</rdfs:comment>
    <rdfs:domain rdf:resource="#Bird"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:ID="eggCount">
    <rdfs:label>egg count</rdfs:label>
    <rdfs:comment>The typical number of eggs the bird lays in a clutch.</rdfs:comment>
    <rdfs:domain rdf:resource="#Bird"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
  </owl:DatatypeProperty>

  <!-- Bird Species Instances -->
  
  <ForestBird rdf:ID="Robin">
    <rdfs:label>Robin</rdfs:label>
    <rdfs:comment>Small songbird that migrates seasonally and is known for its red breast.</rdfs:comment>
    <wingSpan rdf:datatype="http://www.w3.org/2001/XMLSchema#float">25.0</wingSpan>
    <flightAltitude rdf:datatype="http://www.w3.org/2001/XMLSchema#float">50.0</flightAltitude>
    <nestingHeight rdf:datatype="http://www.w3.org/2001/XMLSchema#float">3.0</nestingHeight>
    <eggCount rdf:datatype="http://www.w3.org/2001/XMLSchema#int">4</eggCount>
  </ForestBird>

  <SeaBird rdf:ID="Albatross">
    <rdfs:label>Albatross</rdfs:label>
    <rdfs:comment>Large seabird with exceptional flying abilities found in ocean environments.</rdfs:comment>
    <wingSpan rdf:datatype="http://www.w3.org/2001/XMLSchema#float">320.0</wingSpan>
    <flightAltitude rdf:datatype="http://www.w3.org/2001/XMLSchema#float">1000.0</flightAltitude>
    <nestingHeight rdf:datatype="http://www.w3.org/2001/XMLSchema#float">0.5</nestingHeight>
    <eggCount rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</eggCount>
  </SeaBird>

  <ForestBird rdf:ID="Sparrow">
    <rdfs:label>Sparrow</rdfs:label>
    <rdfs:comment>Small passerine bird found in forests and urban areas worldwide.</rdfs:comment>
    <wingSpan rdf:datatype="http://www.w3.org/2001/XMLSchema#float">20.0</wingSpan>
    <flightAltitude rdf:datatype="http://www.w3.org/2001/XMLSchema#float">30.0</flightAltitude>
    <nestingHeight rdf:datatype="http://www.w3.org/2001/XMLSchema#float">2.0</nestingHeight>
    <eggCount rdf:datatype="http://www.w3.org/2001/XMLSchema#int">5</eggCount>
  </ForestBird>

  <SeaBird rdf:ID="Eagle">
    <rdfs:label>Eagle</rdfs:label>
    <rdfs:comment>Large bird of prey with powerful talons and exceptional eyesight.</rdfs:comment>
    <wingSpan rdf:datatype="http://www.w3.org/2001/XMLSchema#float">220.0</wingSpan>
    <flightAltitude rdf:datatype="http://www.w3.org/2001/XMLSchema#float">3000.0</flightAltitude>
    <nestingHeight rdf:datatype="http://www.w3.org/2001/XMLSchema#float">15.0</nestingHeight>
    <eggCount rdf:datatype="http://www.w3.org/2001/XMLSchema#int">2</eggCount>
  </SeaBird>

  <ForestBird rdf:ID="Owl">
    <rdfs:label>Owl</rdfs:label>
    <rdfs:comment>Nocturnal bird of prey found in forests with excellent night vision.</rdfs:comment>
    <wingSpan rdf:datatype="http://www.w3.org/2001/XMLSchema#float">80.0</wingSpan>
    <flightAltitude rdf:datatype="http://www.w3.org/2001/XMLSchema#float">100.0</flightAltitude>
    <nestingHeight rdf:datatype="http://www.w3.org/2001/XMLSchema#float">8.0</nestingHeight>
    <eggCount rdf:datatype="http://www.w3.org/2001/XMLSchema#int">3</eggCount>
  </ForestBird>

  <!-- Habitat Instances -->
  
  <Ocean rdf:ID="PacificOcean">
    <rdfs:label>Pacific Ocean</rdfs:label>
    <rdfs:comment>The largest ocean on Earth, home to many seabird species.</rdfs:comment>
  </Ocean>

  <Ocean rdf:ID="AtlanticOcean">
    <rdfs:label>Atlantic Ocean</rdfs:label>
    <rdfs:comment>The second largest ocean, providing habitat for coastal and pelagic birds.</rdfs:comment>
  </Ocean>

  <Forest rdf:ID="AmazonRainforest">
    <rdfs:label>Amazon Rainforest</rdfs:label>
    <rdfs:comment>The largest tropical rainforest in the world, home to diverse bird species.</rdfs:comment>
  </Forest>

  <Mountain rdf:ID="Himalayas">
    <rdfs:label>Himalayas</rdfs:label>
    <rdfs:comment>The world's highest mountain range, containing unique high-altitude bird species.</rdfs:comment>
  </Mountain>

  <!-- Property Assertions -->
  
  <rdf:Description rdf:about="#Robin">
    <livesIn rdf:resource="#AmazonRainforest"/>
    <feedsOn>worms, insects, and small fruits</feedsOn>
  </rdf:Description>

  <rdf:Description rdf:about="#Albatross">
    <livesIn rdf:resource="#PacificOcean"/>
    <feedsOn>fish, squid, and marine organisms</feedsOn>
    <flockssWith rdf:resource="#Albatross"/>
  </rdf:Description>

  <rdf:Description rdf:about="#Sparrow">
    <livesIn rdf:resource="#AmazonRainforest"/>
    <feedsOn>seeds, insects, and small berries</feedsOn>
  </rdf:Description>

  <rdf:Description rdf:about="#Eagle">
    <livesIn rdf:resource="#Himalayas"/>
    <feedsOn>fish, small mammals, and other birds</feedsOn>
  </rdf:Description>

  <rdf:Description rdf:about="#Owl">
    <livesIn rdf:resource="#AmazonRainforest"/>
    <feedsOn>small mammals, insects, and other birds</feedsOn>
  </rdf:Description>

</rdf:RDF>